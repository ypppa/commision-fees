{
    "name": "ypppa/commission-fees",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Ypppa\\CommissionFees\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ypppa\\CommissionFees\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Yuri Gaidoba",
            "email": "yuri.gaidoba@paysera.net"
        }
    ],
    "require": {
        "php": "^8.0",
        "evp/money": "^2.6",
        "ext-bcmath": "*",
        "paysera/lib-normalization": "^1.3",
        "symfony/console": "^6.0",
        "symfony/validator": "^5.4",
        "symfony/yaml": "^6.0",
        "symfony/dependency-injection": "^5.4",
        "halaxa/json-machine": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.21",
        "symfony/var-dumper": "^6.0",
        "symfony/test-pack": "^1.1"
    },
    "scripts": {
        "check-cs": [
            "./vendor/bin/php-cs-fixer list-files"
        ],
        "fix-cs": [
            "./vendor/bin/php-cs-fixer fix"
        ]
    }
}
